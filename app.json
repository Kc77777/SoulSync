{
  "expo": {
    "name": "soulsync-app",
    "slug": "soulsync-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/cosmic-background.png",
      "resizeMode": "cover",
      "backgroundColor": "#0b0f1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aavaskc.soulsyncapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "devDependencies": {
      "@react-native-community/cli": "latest"
    },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b0f1a"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7957c130-bdad-46f8-a3d7-311992059c0e"
      }
    },
    "plugins": ["expo-asset"]
  }
}
